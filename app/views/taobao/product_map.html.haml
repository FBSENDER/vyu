- title("淘宝商品大全")
- head_desc("淘宝商品大全 - vxixi淘宝商品站点地图")
- keywords("淘宝商品大全")
- path("/product_map/")
- mobile_url("http://www.vxixi.com#{@path}")

.container
  .row
    .col-md-12
      %nav
        %a{href: "/"} 羽毛球
        &nbsp;>&nbsp;
        %b 淘宝商品大全

  %article.row
    .col-md-12
      %h1 淘宝商品大全
      %p
        - @products.each do |pd|
          %a{href: "/baobei/#{pd.item_id}/"} #{pd.title}
      %p
        -((@page+1)..(@page + 21)).each do |num|
          %a{href: "/product_map/?page=#{num}"} 第#{num}页
      .page-header
      %footer
        %div#SOHUCS
      .page-header
